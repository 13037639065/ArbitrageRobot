# 情将config.yaml.example重命名为config.yaml，并填入你的API密钥。系统会自动在家对应api_key和api_secret。
# 请不要将key和secret泄露给他人，以免造成损失。也不上上传到代码仓库中。

exchanges:
  # 币安
  binance:
    api_key: your_api_key 
    api_secret: your_api_secret

  # 欧意
  okx:
    api_key: your_api_key 
    api_secret: your_api_secret

  # 火币
  htx:
    api_key: your_api_key 
    api_secret: your_api_secret

  # bitget
  bitget:
    api_key: your_api_key 
    api_secret: your_api_secret
    password: your_password

# webhook URL
webhook: 'https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your_webhook_key'